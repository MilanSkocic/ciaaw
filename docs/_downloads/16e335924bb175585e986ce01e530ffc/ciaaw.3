/* EXAMPLE IN C */
#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <stdbool.h>
#include \(dqciaaw.h\(dq


    // ASAW = Abridged Standard Atomic Weight
    // SAW  = Standard Atomic Weight
    // ICE  = Isotopic Composition of the Element
    // NAW  = Nuclide Atomic Weight
    // U    = Uncertainty

int main(void){


    printf(\(dq%s\en\(dq, \(dq########## CIAAW VERSION ##########\(dq);
    printf(\(dqversion %s\en\(dq, ciaaw_get_version());
    
    printf(\(dq%s\en\(dq, \(dq########## CIAAW SAW ##########\(dq);
    printf(\(dq%s %10.5f\en\(dq, \(dqASAW H   = \(dq, ciaaw_get_saw(\(dqH\(dq, 1, true, false));
    printf(\(dq%s %10.5f\en\(dq, \(dqU ASAW H = \(dq, ciaaw_get_saw(\(dqH\(dq, 1, true, true));
    printf(\(dq%s %10.5f\en\(dq, \(dqSAW H    = \(dq, ciaaw_get_saw(\(dqH\(dq, 1, false, false));
    printf(\(dq%s %10.5f\en\(dq, \(dqU SAW H  = \(dq, ciaaw_get_saw(\(dqH\(dq, 1, false, true));
    printf(\(dq%s %10.5f\en\(dq, \(dqASAW Tc  = \(dq, ciaaw_get_saw(\(dqTc\(dq, 2, true, false));

    printf(\(dq%s\en\(dq, \(dq########## CIAAW ICE ##########\(dq);
    printf(\(dq%s %d\en\(dq,     \(dqN ICE H      = \(dq, ciaaw_get_nice(\(dqH\(dq, 1));
    printf(\(dq%s %12.6f\en\(dq, \(dqICE H 1      = \(dq, ciaaw_get_ice(\(dqH\(dq, 1, 1, false)); 
    printf(\(dq%s %23.16e\en\(dq,\(dqU ICE H 1    = \(dq, ciaaw_get_ice(\(dqH\(dq, 1, 1, true)); 
    printf(\(dq%s %12.6f\en\(dq, \(dqICE H 2      = \(dq, ciaaw_get_ice(\(dqH\(dq, 1, 2, false)); 
    printf(\(dq%s %23.16e\en\(dq,\(dqU ICE H 2    = \(dq, ciaaw_get_ice(\(dqH\(dq, 1, 2, true)); 
    printf(\(dq%s %d\en\(dq,     \(dqN ICE Tc     = \(dq, ciaaw_get_nice(\(dqTc\(dq, 2));
    printf(\(dq%s %d\en\(dq,     \(dqN ICE C      = \(dq, ciaaw_get_nice(\(dqC\(dq, 1));

    printf(\(dq%s\en\(dq, \(dq########## CIAAW NAW ##########\(dq);
    printf(\(dq%s %23.16f\en\(dq, \(dqNAW H 2      = \(dq, ciaaw_get_naw(\(dqH\(dq, 1, 2, false)); 
    printf(\(dq%s %23.16e\en\(dq, \(dqU NAW H 2    = \(dq, ciaaw_get_naw(\(dqH\(dq, 1, 2, true)); 
    printf(\(dq%s %d\en\(dq,      \(dqN NAW Tc     = \(dq, ciaaw_get_nnaw(\(dqTc\(dq, 2));

    return EXIT_SUCCESS;
    }

