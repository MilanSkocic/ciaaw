.sp
.EX
! EXAMPLE IN FORTRAN
program example_in_f
    use ciaaw
    implicit none

    character(len=8) :: s
    
    ! ASAW = Abridged Standard Atomic Weight
    ! SAW  = Standard Atomic Weight
    ! ICE  = Isotopic Composition of the Element
    ! NAW  = Nuclide Atomic Weight
    ! U    = Uncertainty

    print \(aq(A)\(aq, \(aq########### CIAAW VERSION ##########\(aq
    print *, \(dqversion \(dq, get_version()  

    print \(aq(A)\(aq, \(aq########### CIAAW SAW ##########\(aq
    print \(aq(A10, F10.5)\(aq, \(aqASAW H   = \(aq, get_saw(\(dqH\(dq, abridged=.true.)
    print \(aq(A10, F10.5)\(aq, \(aqU ASAW H = \(aq, get_saw(\(dqH\(dq, uncertainty=.true.)
    print \(aq(A10, F10.5)\(aq, \(aqSAW H    = \(aq, get_saw(\(dqH\(dq, abridged = .false.)
    print \(aq(A10, F10.5)\(aq, \(aqU SAW H  =  \(aq, get_saw(\(dqH\(dq, abridged = .false., uncertainty = .true.)
    print \(aq(A10, F10.5)\(aq, \(aqASAW T   = \(aq, get_saw(\(dqTc\(dq, abridged=.true.)
    
    print \(aq(A)\(aq, \(aq########### CIAAW ICE ##########\(aq
    print \(aq(A, I3)\(aq,        \(aqN ICE H    = \(aq, get_nice(\(dqH\(dq)
    print \(aq(A, F12.6)\(aq,   \(aqICE H 1    = \(aq, get_ice(\(dqH\(dq, A=1)
    print \(aq(A, ES23.16)\(aq, \(aqU ICE H 1  = \(aq, get_ice(\(dqH\(dq, A=1, uncertainty=.true.)
    print \(aq(A, F12.6)\(aq,   \(aqICE H 2    = \(aq, get_ice(\(dqH\(dq, A=2)
    print \(aq(A, ES23.16)\(aq, \(aqU ICE H 2  = \(aq, get_ice(\(dqH\(dq, A=2, uncertainty=.true.)
    print \(aq(A, I3)\(aq,        \(aqN ICE Tc   = \(aq, get_nice(\(dqTc\(dq)
    print \(aq(A, I3)\(aq,        \(aqN ICE C    = \(aq, get_nice(\(dqC\(dq)
    
    print \(aq(A)\(aq, \(aq########### CIAAW NAW ##########\(aq
    print \(aq(A, ES23.16)\(aq,   \(aqNAW H 2     = \(aq, get_naw(\(dqH\(dq, A=2)
    print \(aq(A, ES23.16)\(aq,   \(aqU NAW H 2   = \(aq, get_naw(\(dqH\(dq, A=2, uncertainty=.true.)
    print \(aq(A, I3)\(aq,          \(aqN NAW Tc    = \(aq, get_nnaw(\(dqTc\(dq)

end program

